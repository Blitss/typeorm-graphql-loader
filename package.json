{
  "name": "typeorm-loader",
  "version": "0.0.1",
  "description": "A naive ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --declaration",
    "lint": "tslint -c tslint.json --project tsconfig.json 'src/**/*.ts'",
    "test": "yarn run build && [ -d __tests__ ] && NODE_ENV=test nyc mocha -r ts-node/register -r tslib -r source-map-support/register --full-trace --bail __tests__/**/*.test.ts",
    "test-with-coverage": "yarn test && cat ./coverage/lcov.info | codacy-coverage"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "tslib"
    ],
    "reporter": [
      "lcovonly",
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/Mando75/typeorm-graphql-loader"
  },
  "keywords": [
    "typeorm",
    "database",
    "graphql",
    "data",
    "loader",
    "batching",
    "caching"
  ],
  "author": "Bryan Muller",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/Mando75/typeorm-graphql-loader/issues"
  },
  "homepage": "https://gitlab.com/Mando75/typeorm-graphql-loader",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chance": "^1.0.1",
    "@types/deep-equal": "^1.0.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.1",
    "@types/object-path": "^0.9.29",
    "chai": "^4.2.0",
    "chance": "^1.0.16",
    "codacy-coverage": "^3.4.0",
    "deep-equal-in-any-order": "^1.0.18",
    "graphql": "^14.0.2",
    "metanoia": "^1.0.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "sqlite3": "^4.0.3",
    "ts-node": "^7.0.1",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typeorm": "^0.2.8",
    "typescript": "^3.1.3"
  },
  "peerDependencies": {
    "typeorm": ">=0.2.8"
  },
  "optionalDependencies": {
    "@types/graphql": "^14.0.3"
  }
}
